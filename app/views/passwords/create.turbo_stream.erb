<%= turbo_stream.prepend "passwords" do %>
  <%= render @password %>
<% end %>

<%= turbo_stream.update :modal do %>
  <%= turbo_frame_tag :modal do %>
    <%= render Shared::ModalComponent.new do %>
      <%= render partial: 'passwords/show', locals: { password: @password } %>
    <% end %>
  <% end %>
<% end %>
